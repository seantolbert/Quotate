<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Quotate</title>
  </head>
  <body>
    <h1>All Quotes</h1>
    <div id="list">
      <blockquote>
        <% quotes.forEach(function(q) { %>
          <p><%= q.content %></p>
          -
          <cite><%= q.source %></cite>
          <button><a href="/quotes/<%= q._id %>"><small>Preferences</small></a></button>
        <% }); %>
      </blockquote>
    </div>
  </body>
</html>
